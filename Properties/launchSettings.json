{
	"profiles": {
		"Terraria": {
			"commandName": "Executable",
			"executablePath": "$(DotNetName)",
			"commandLineArgs": "$(tMLPath)",
			"workingDirectory": "$(tMLSteamPath)"
		},
		"TerrariaSkipSelect": {
			"commandName": "Executable",
			"executablePath": "$(DotNetName)",
			"commandLineArgs": "$(tMLPath) -skipselect",
			"workingDirectory": "$(tMLSteamPath)"
		},
		"Terraria (fast start)": {
			"commandName": "Project",
			"commandLineArgs": "\"$(TerrariaSteamPath)$(tMLPath)\""
		},
		"TerrariaServer": {
			"commandName": "Executable",
			"executablePath": "$(DotNetName)",
			"commandLineArgs": "$(tMLServerPath)",
			"workingDirectory": "$(tMLSteamPath)"
		}
	}
}